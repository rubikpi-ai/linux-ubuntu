};

&ufs_mem_phy {
	vdda-phy-supply = <&vreg_l10c_0p88>;
	vdda-pll-supply = <&vreg_l6b_1p2>;

	status = "okay";
};

&tlmm {
	wifi_reset_active: wifi_reset_active {
		pins = "gpio16";
		function = "gpio";
		drive-strength = <2>;
		output-high;
		bias-disable;
	};
};

/ {
	vreg_sdio_wifi_1p8: vreg_sdio_wifi_1p8 {
		compatible = "regulator-fixed";
		regulator-name = "vreg_sdio_wifi_1p8";
		enable-active-high;
		gpio = <&tlmm 125 GPIO_ACTIVE_HIGH>;
		regulator-always-on;
		regulator-enable-ramp-delay = <50000>;
	};

	vreg_sdio_bt_reset_1p8: vreg_sdio_bt_reset_1p8 {
		pinctrl-0 = <&wifi_reset_active>;
		pinctrl-names = "default";
		compatible = "regulator-fixed";
		regulator-name = "vreg_sdio_bt_reset_1p8";
		regulator-always-on;
		enable-active-high;
		gpio = <&tlmm 17 GPIO_ACTIVE_HIGH>;
		vin-supply = <&vreg_sdio_wifi_1p8>;
	};

	bcmdhd_wlan: bcmdhd_wlan {
		status = "okay";
		compatible = "linux,bcmdhd_wlan";
		gpio_wl_reg_on = <&tlmm 16 GPIO_ACTIVE_HIGH>;
		gpio_wl_host_wake = <&tlmm 38 GPIO_ACTIVE_HIGH>;
		vdda-supply = <&vreg_sdio_bt_reset_1p8>;
	};
};

&sdhc_2 {
	status = "okay";

	pinctrl-0 = <&sdc2_clk>, <&sdc2_cmd>, <&sdc2_data>;
	pinctrl-1 = <&sdc2_clk_sleep>, <&sdc2_cmd_sleep>, <&sdc2_data_sleep>;

	vmmc-supply = <&vreg_l9c_2p96>;
	vqmmc-supply = <&vreg_l6c_2p96>;

	non-removable;
	keep-power-in-suspend;

	/delete-property/ cd-gpios;
};

/delete-node/ &usb_1_dwc3;

&usb_1 {

	reg = <0x0 0x0a600000 0x0 0x200000>;

	interrupts-extended = <&intc GIC_SPI 133 IRQ_TYPE_LEVEL_HIGH>,
			      <&intc GIC_SPI 131 IRQ_TYPE_LEVEL_HIGH>,
			      <&pdc 14 IRQ_TYPE_LEVEL_HIGH>,
			      <&pdc 15 IRQ_TYPE_EDGE_BOTH>,
			      <&pdc 17 IRQ_TYPE_EDGE_BOTH>;
	interrupt-names = "dwc_usb3",
			  "hs_phy_irq",
			  "dp_hs_phy_irq",
			  "dm_hs_phy_irq",
			  "ss_phy_irq";

	iommus = <&apps_smmu 0xe0 0x0>;
	snps,dis_u2_susphy_quirk;
	snps,dis_enblslpm_quirk;
	phys = <&usb_1_hsphy>, <&usb_1_qmpphy QMP_USB43DP_USB3_PHY>;
	phy-names = "usb2-phy", "usb3-phy";
	maximum-speed = "super-speed";
	snps,dis-u2-entry-quirk;
	snps,dis-u1-entry-quirk;
	dr_mode = "otg";
	usb-role-switch;
	qcom,enable-rt;
	wakeup-source;

	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			usb_1_dwc3_hs: endpoint {
			};
		};
	};
};

&usb_1_dwc3_hs {
	remote-endpoint = <&pmic_glink_hs_in>;
};

&usb_1_hsphy {
	vdda-pll-supply = <&vreg_l10c_0p88>;
	vdda18-supply = <&vreg_l1c_1p8>;
	vdda33-supply = <&vreg_l2b_3p072>;

	status = "okay";
};

&usb_1_qmpphy {
	vdda-phy-supply = <&vreg_l6b_1p2>;
	vdda-pll-supply = <&vreg_l1b_0p912>;

	orientation-switch;
	status = "okay";
};

&vreg_l7b_2p952 {
	/delete-property/regulator-min-microvolt;
	/delete-property/regulator-max-microvolt;
	regulator-allow-set-load;
	regulator-allowed-modes = <RPMH_REGULATOR_MODE_LPM RPMH_REGULATOR_MODE_HPM>;
};

&vreg_l9b_1p2 {
	/delete-property/regulator-min-microvolt;
	/delete-property/regulator-max-microvolt;
	regulator-allow-set-load;
	regulator-allowed-modes = <RPMH_REGULATOR_MODE_LPM RPMH_REGULATOR_MODE_HPM>;
};

/*     USB2 Related Start        */
/*==============================*/

&pm7325_gpios {
	usb2_vbus_det_default: usb2_vbus_det_state {
		pins = "gpio9";
		function = "normal";
		bias-pull-down;
		input-enable;
		power-source = <0>;
	};
};

&sdc2_clk {
	bias-disable;
	drive-strength = <16>;
};

&sdc2_cmd {
	bias-pull-up;
	drive-strength = <10>;
};

&sdc2_data {
	bias-pull-up;
	drive-strength = <10>;
};

&tlmm {
	usb2_1p8_vreg_default: usb2_1p8_vreg_default {
		pins = "gpio119";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
		output-high;
	};
};

/delete-node/ &usb_2_dwc3;

&usb_2 {
	pinctrl-0 = <&usb2_1p8_vreg_default>;
	pinctrl-names = "default";

	reg = <0x0 0x08c00000 0x0 0x100000>;

	interrupts-extended = <&intc GIC_SPI 242 IRQ_TYPE_LEVEL_HIGH>,
				<&intc GIC_SPI 240 IRQ_TYPE_LEVEL_HIGH>,
				<&pdc 12 IRQ_TYPE_LEVEL_HIGH>,
				<&pdc 13 IRQ_TYPE_EDGE_BOTH>;
	interrupt-names = "dwc_usb2",
			"hs_phy_irq",
			"dp_hs_phy_irq",
			"dm_hs_phy_irq";

	iommus = <&apps_smmu 0xa0 0x0>;
	snps,dis_u2_susphy_quirk;
	snps,dis_enblslpm_quirk;
	phys = <&usb_2_hsphy>;
	phy-names = "usb2-phy";
	maximum-speed = "high-speed";
	snps,dis-u2-entry-quirk;
	snps,dis-u1-entry-quirk;

	usb-role-switch;
	qcom,enable-rt;
	wakeup-source;
	dr_mode = "host";

	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			usb2_role_switch: endpoint {
				remote-endpoint = <&eud_ep>;
			};
		};
	};
};

&usb_2_hsphy {
	vdda-pll-supply = <&vreg_l10c_0p88>;
	vdda18-supply = <&vreg_l1c_1p8>;
	vdda33-supply = <&vreg_l2b_3p072>;

	status = "okay";
};

&spmi_bus {
	pmic@PM7250B_SID {
		pm7250b_bcl: bcl@1d00 {
			compatible = "qcom,bcl-v5";
			reg = <0x1d00>;
			interrupts = <PM7250B_SID 0x1d 0x0 IRQ_TYPE_EDGE_RISING>,
					<PM7250B_SID 0x1d 0x1 IRQ_TYPE_EDGE_RISING>,
					<PM7250B_SID 0x1d 0x2 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "bcl-lvl0",
						"bcl-lvl1",
						"bcl-lvl2";
			#thermal-sensor-cells = <1>;
		};
	};
};

&thermal_zones {
	pm7250b-ibat-lvl0 {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-sensors = <&pm7250b_bcl 0>;

		trips {
			ibat_lvl0:ibat-lvl0 {
				temperature = <6000>;
				hysteresis = <200>;
				type = "passive";
			};
