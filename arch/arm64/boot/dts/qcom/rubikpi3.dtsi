		reg = <0x0 0x0 0x0 0x0 0x0>;
		#address-cells = <5>;
		#size-cells = <0>;
		pcie1_bus1_dev0_fn0: pcie1_bus1_dev0_fn0 {
			reg = <0 0 0 0 0>;
		};
	};
